source 'https://cdn.cocoapods.org/'

require Pod::Executable.execute_command('node', ['-p',
  'require.resolve("react-native/scripts/react_native_pods.rb", { paths: [process.argv[1]] })',
  __dir__
]).strip

platform :ios, '16.0'
ENV['RCT_NEW_ARCH_ENABLED'] = '0'
prepare_react_native_project!

install! 'cocoapods', :integrate_targets => true, :deterministic_uuids => false

use_frameworks! :linkage => :static
use_modular_headers!

# âœ… Stable SDK pins
$FirebaseSDKVersion = '10.22.0'
$PromisesObjCVersion = '2.4.0'

target 'StylIQ' do
  config = use_native_modules!

  use_react_native!(
    path: config[:reactNativePath],
    hermes_enabled: false,
    fabric_enabled: false,
    app_path: "#{Pod::Config.instance.installation_root}/.."
  )

  # --- Firebase Core Stack ---
  pod 'Firebase/CoreOnly',   $FirebaseSDKVersion
  pod 'Firebase/Auth',       $FirebaseSDKVersion
  pod 'Firebase/Messaging',  $FirebaseSDKVersion
  pod 'Firebase/Analytics',  $FirebaseSDKVersion
  pod 'Firebase/Storage',    $FirebaseSDKVersion
  pod 'Firebase/Firestore',  $FirebaseSDKVersion
  pod 'FirebaseCrashlytics', $FirebaseSDKVersion

  # --- Promises modules ---
  pod 'PromisesObjC',  $PromisesObjCVersion
  pod 'PromisesSwift', $PromisesObjCVersion

  # --- Core React Native pod ---
  pod 'glog', :podspec => '../node_modules/react-native/third-party-podspecs/glog.podspec'

  # âœ… Single authoritative post_install
  post_install do |installer|
    react_native_post_install(
      installer,
      config[:reactNativePath],
      :mac_catalyst_enabled => false
    )

    installer.pods_project.targets.each do |t|
      t.build_configurations.each do |cfg|
        s = cfg.build_settings
        s['IPHONEOS_DEPLOYMENT_TARGET'] = '16.0'
        s['CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES'] = 'YES'

        # âœ… Make key Firebase deps modular
        if ['PromisesObjC', 'PromisesSwift', 'GoogleUtilities'].include?(t.name)
          s['DEFINES_MODULE'] = 'YES'
          s['BUILD_LIBRARY_FOR_DISTRIBUTION'] = 'YES'
        end

        # ðŸ”¹ Strip invalid -G flags completely for BoringSSL & gRPC
        if ['BoringSSL-GRPC', 'gRPC-Core', 'gRPC-C++'].include?(t.name)
          %w[OTHER_CFLAGS OTHER_CPLUSPLUSFLAGS OTHER_LDFLAGS].each do |flag_type|
            arr = Array(s[flag_type]).compact.map(&:to_s)
            arr.reject! { |f| f.strip == '-G' || f.include?('-G ') }
            s[flag_type] = arr.uniq
          end

          # ðŸ”¹ Ensure none sneak back in from inherited configs
          s['OTHER_CFLAGS[sdk=iphonesimulator*]'] = '$(inherited)'
          s['OTHER_CPLUSPLUSFLAGS[sdk=iphonesimulator*]'] = '$(inherited)'
          s['OTHER_LDFLAGS[sdk=iphonesimulator*]'] = '$(inherited)'

          # ðŸ”¹ Disable BoringSSL ASM on simulator
          s['GCC_PREPROCESSOR_DEFINITIONS'] ||= ['$(inherited)']
          s['GCC_PREPROCESSOR_DEFINITIONS'] << 'OPENSSL_NO_ASM=1'
        end

        # ðŸ§© NEW FIX: force gRPC to compile as C++17 (Xcode 16 compatibility)
        if ['gRPC-Core', 'gRPC-C++'].include?(t.name)
          s['CLANG_CXX_LANGUAGE_STANDARD'] = 'c++17'
          s['CLANG_CXX_LIBRARY'] = 'libc++'
        end
      end
    end

    # âœ… Optional extra safety if you want to force-clean modules
    if File.exist?("ios/fix_boringssl.sh")
      system("bash ios/fix_boringssl.sh")
    end

    installer.pods_project.save
  end
end









